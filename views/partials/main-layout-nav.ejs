            d="M235.54 150.21a104.84 104.84 0 0 1-37 52.91A104 104 0 0 1 32 120a103.09 103.09 0 0 1 20.88-62.52a104.84 104.84 0 0 1 52.91-37a8 8 0 0 1 10 10a88.08 88.08 0 0 0 109.8 109.8a8 8 0 0 1 10 10Z" />
        </svg>
      </button>
      <button
        type="button"
        class="flex mx-3 text-sm bg-gray-800 rounded-full md:mr-0 focus:ring-4 focus:ring-gray-300 dark:focus:ring-gray-600"
        id="user-menu-button"
        aria-expanded="false"
        data-dropdown-toggle="dropdown">
        <span class="sr-only">Open user menu</span>
        <div
          class="overflow-hidden bg-indigo-200 w-7 h-7 rounded-full flex justify-center items-center border-2 border-indigo-400">
          <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5 text-indigo-600" viewBox="0 0 24 24">
            <path
              fill="currentColor"
              fill-rule="evenodd"
              d="M7.5 6a4.5 4.5 0 1 1 9 0a4.5 4.5 0 0 1-9 0M3.751 20.105a8.25 8.25 0 0 1 16.498 0a.75.75 0 0 1-.437.695A18.683 18.683 0 0 1 12 22.5c-2.786 0-5.433-.608-7.812-1.7a.75.75 0 0 1-.437-.695"
              clip-rule="evenodd" />
          </svg>
        </div>
      </button>
      <!-- Dropdown menu -->
      <div
        class="hidden z-50 my-4 w-56 text-base list-none bg-white rounded divide-y divide-gray-100 shadow dark:bg-gray-700 dark:divide-gray-600"
        id="dropdown">
        <div class="py-3 px-4">
          <span class="block text-sm font-semibold text-gray-900 dark:text-white"><%= accName %></span>
          <% if (role === 'sudo') { %>
          <span
            class="bg-gray-100 text-gray-800 text-xs font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-gray-400 border border-gray-500"
            >Sudo</span
          >

          <% } else { %>
          <span
            class="bg-blue-100 text-blue-800 text-xs font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-blue-400 border border-blue-400"
            ><%= role %></span
          >
          <% } %>
        </div>
        <ul class="py-1 text-gray-700 dark:text-gray-300" aria-labelledby="dropdown">
          <% if (role === 'sudo') { %>
          <li>
            <a
              href="/credential"
              class="block py-2 px-4 text-sm hover:bg-gray-100 dark:hover:bg-gray-600 dark:text-gray-400 dark:hover:text-white credential_link"
              >Ubah Password</a
            >
          </li>
          <% } %>
          <li>
            <a
              onClick="logOut()"
              href="#"
              class="block py-2 px-4 text-sm hover:bg-gray-100 dark:hover:bg-gray-600 dark:hover:text-white"
              >Keluar</a
            >
          </li>
        </ul>
      </div>
    </div>
  </div>
</nav>
<script>
  const htmlEl = document.querySelector('html')
  const isThemeSet = localStorage.getItem('theme')
  if (!isThemeSet) {
    localStorage.setItem('theme', null)
  }
  const setTheme = () => {
    const getThemeInfo = localStorage.getItem('theme')

    if (getThemeInfo === 'dark') {
      htmlEl.classList.add('dark')
    } else {
      htmlEl.classList.remove('dark')
    }
  }
  setTheme()
  const changeTheme = () => {
    const themeInfo = localStorage.getItem('theme')
    if (themeInfo !== 'dark') {
      localStorage.setItem('theme', 'dark')
    } else {
      localStorage.setItem('theme', null)
    }
    setTheme()
  }
</script>
